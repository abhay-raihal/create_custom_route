        "kint-php/kint": "2.*",
        "laracasts/testdummy": "dev-laravel_9",
        "mockery/mockery": "*",
        "tijsverkoyen/css-to-inline-styles": "2.*",
        "php-http/mock-client": "^1.4.1",
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "4.4.*",
        "orchestra/testbench": "7.0.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "roave/no-leaks": "1.4.x-dev",
        "dg/bypass-finals": "^1.4"
    },
    "autoload": {
        "classmap": [
            "database",
            "tests",
            "app/Models/Customer"
        ],
        "psr-4": {
            "RZP\\": "app/",
            "": "generated/proto/",
            "AuthzAdmin\\Client\\": "generated/proto/authz/admin/v1/SwaggerClient-php/lib",
            "Swagger\\Client\\": "generated/proto/authz/enforcer/v1/SwaggerClient-php/lib",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Migrations\\": "database/migrations/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('environment/.env.sample', 'environment/.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ],
        "proto-gen": [
            "scripts/proto-gen.sh"
        ],
        "proto-clean": [
            "rm -rf generated/proto"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
